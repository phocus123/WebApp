 	<div id = "outer">
		<header>
			    <nav class = "nav-height">
				    <div class="nav-wrapper #448aff blue accent-2">
				      <a href="" class="brand-logo"><div class = "image-margins"><%= image_tag("newlogo.png") %></div></a>
				      <a href="" data-activates="mobile-demo" class="button-collapse"><i class="material-icons">menu</i></a>
				      <ul class="right hide-on-med-and-down">
				        <li class = "shutter-out-horizontal"><%= link_to('Log Out', logout_path)%></li>
				        <li class = "shutter-out-horizontal"><a href="" >Contact</a></li>
				        <li class = "shutter-out-horizontal"><a href="">About</a></li>
				      </ul>
				      <ul class="side-nav" id="mobile-demo">
				        <li><%= link_to('Home', home_path)%></li>
				        <li><a href="">Contact</a></li>
				        <li><a href="">About</a></li>
				      </ul>
				    </div>
				  </nav>
		</header>
		
		<main class = "center-align full-height main">
	      <h1 class = "t-shadow">Whiteboard</h1>
	      <h4 class = "t-shadow">Welcome <%= @user.first_name %></h4>

			<% if current_user.is_admin? %>
		        <div class = "container">
			   		<table class = "striped"> 
			   			<h5 class = "t-shadow">Students</h5>                        
						  <tr>
						  	<th>ID</th>                          
						    <th>First Name</th>
						    <th>Last Name</th>
						    <th>User Name</th>
						    <th></th>
						    <th></th>
						  </tr>
						<% @users.each do |user| %>       
						  <tr>
						  	<td><p><%= user.id %></p></td> 
						    <td><p><%= user.first_name %></p></td> 
						    <td><p><%= user.last_name%></p></td>
						    <td><p><%= user.username%></p></td> 
						    <td><p><%= link_to "Edit", edit_user_path(user) %></p></td> 
						    <td><%= link_to "Delete", user, method: :delete, data: { confirm: "Are you sure?" } %></td>   
						  </tr>
						<% end %>                        
					</table>
					<td><p><%= link_to "Create new user", new_user_path %></p></td> 

					<h5 class = "t-shadow">Courses</h5>  

					<table class = "striped">                         
						  <tr>                          
						    <th>Course ID</th>
						    <th>Course Name</th>
						    <th></th>
						    <th></th>
						  </tr>
						<% @courses.each do |c| %>       
						  <tr>
						    <td><p><%= c.id %></p></td> 
						    <td><p><%= c.course_name %></p></td>
						    <td><p><%= link_to "Edit", edit_course_path(c) %></p></td>
						    <td><%= link_to "Delete", c, method: :delete, data: { confirm: "Are you sure?" } %></td>   
						  </tr>
						<% end %>                     
					</table>
					<td><p><%= link_to "Create new course", new_course_path %></p></td>   

					<h5 class = "t-shadow">Content</h5>  

					<table class = "striped">                         
						  <tr>                          
						    <th>Content ID</th>
						    <th>Content </th>
						    <th></th>
						    <th></th>
						  </tr>
						<% @contents.each do |co| %>       
						  <tr>
						    <td><p><%= co.id %></p></td> 
						    <td><p><%= co.content_one %></p></td>
						    <td><p><%= link_to "Edit", edit_content_path(co) %></p></td>
						    <td><%= link_to "Delete", co, method: :delete, data: { confirm: "Are you sure?" } %></td>   
						  </tr>
						<% end %>                    
					</table>
					<td><p><%= link_to "Create new content", new_content_path %></p></td>    
				</div>
		    <% end %>

		    <% if current_user.is_admin? == false %>
		    	<table class = "container">
		    		<tr>                          
					    <th>Course Name</th>
					</tr>
		    		<% @user_courses.each do |uc| %>
		    		   <tr>
							<td><p><%= link_to uc.course_name, user_content_path(@user, uc.id) %></p></td>	
						</tr>
					<% end %>  
		    	</table>
		   	<% end %>
			<br class = "clearfloat" />
		</main>
		
		<footer class="page-footer #448aff blue accent-2 center-align">
	          <div class="container center-align">
	              <a class="icon-facebook" href="#"></a>
	              <a class="icon-deviantart" href="#"></a>
	              <a class="icon-digg" href="#"></a>
	              <a class="icon-forrst" href="#"></a>
                  <a class="icon-lastfm" href="#"></a>
	          </div>
	          <div class="footer-copyright">
	            <div class="container">
	            Â© 2017 Copyright Text
	            </div>
	          </div>
        </footer>
	</div>
	